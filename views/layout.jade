doctype 5
html
  head
    title
      = ["Sequelize", title].join(" - ")
    link(rel="shortcut icon", type="image/x-icon", href="/favicon.ico")
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(rel="stylesheet", href="http://yandex.st/highlightjs/7.5/styles/ir_black.min.css")
    script(src="http://yandex.st/highlightjs/7.5/highlight.min.js")
    script hljs.initHighlightingOnLoad();

  body
    .navbar.navbar-default.navbar-fixed-top(role='navigation')
      .container
        .navbar-header
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='/')
            img(src="/images/logo-small.png")
            span Sequelize
        .navbar-collapse.collapse
          ul.nav.navbar-nav
          ul.nav.navbar-nav.navbar-right
            li(class=(activeNavItem === 'home' ? 'active' : ''))
              a(href='/') Home
            li(class=(activeNavItem === 'docs' ? 'active' : ''))
              a(href='/docs') Documentation
            li(class=(activeNavItem === 'articles' ? 'active' : ''))
              a(href='../navbar-fixed-top/') Articles
            li(class=(activeNavItem === 'changelog' ? 'active' : ''))
              a(href='../navbar-fixed-top/') Changelog
            li(class=(activeNavItem === 'blog' ? 'active' : ''))
              a(href='../navbar-fixed-top/') Blog
    block content
